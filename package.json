{
  "name": "webpack-demo",
  "version": "2.0.9",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "ie > 7"
  ],
  "scripts": {
    "deploy": "gh-pages -d build",
    "build:plugin": "webpack --config webpack.plugin.babel.js",
    "test:jest:watch": "jest --watch",
    "test:jest": "jest",
    "test:karma:watch": "karma start --auto-watch --no-single-run",
    "test:karma": "karma start",
    "test:mocha:watch": "webpack-dev-server --hot --config webpack.mocha.js",
    "test:mocha": "mocha tests",
    "build:i18n": "webpack --config webpack.i18n.js",
    "build:ssr": "webpack --config webpack.ssr.js",
    "prebuild:lib": "yarn lint:js && rimraf dist2/*",
    "build:lib": "webpack --config webpack.lib.babel.js",
    "stats": "webpack --env production --profile --json > stats.json",
    "start2": "nodemon --watch webpack.config.babel.js --exec \"webpack-dev-server --env development\"",
    "build": "webpack --env production",
    "lint:style": "stylelint app/**/*.css",
    "lint:js": "eslint app/ lib2/ tests/ loaders/ plugins/ webpack.*.js karma.conf.js run-loader.js --cache",
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src --ext .js,.jsx",
    "test": "yarn lint && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test"
  },
  "devDependencies": {
